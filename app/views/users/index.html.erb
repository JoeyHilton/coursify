  <div class="col-md-6 col-md-offset-3">
    <h1>Users</h1>
    <% @users.each do |user| %>
    <div class='well'>
      <h2><%= user.name %></h2>
      <h3><%= user.email %></h3>
     
      <%= button_to 'Show', user_path(user.id), class: "btn btn-primary", method: :get %>
    </div>
      <br />
    <% end %>
  </div>  